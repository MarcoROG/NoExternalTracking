{

  "manifest_version": 2,
  "name": "NoExternalTracking",
  "version": "1.0",

  "description": "Blocks external tracking from evil websites.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.messenger.com/*"],
      "js": ["NoExternalTracking.js"]
    }
  ]

}